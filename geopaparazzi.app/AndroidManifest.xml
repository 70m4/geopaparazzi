<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="eu.hydrologis.geopaparazzi" android:versionCode="5" android:versionName="1.1.1">
	<application android:icon="@drawable/icon" android:label="@string/app_name"
		android:debuggable="false">
		<activity android:label="@string/app_name" android:name=".GeoPaparazziActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity android:name=".camera.CameraActivity">
			<intent-filter>
				<action android:name="eu.hydrologis.geopaparazzi.camera.TAKE_PICTURE" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".gps.NoteActivity"
			android:configChanges="keyboardHidden|orientation">
			<intent-filter>
				<action android:name="eu.hydrologis.geopaparazzi.camera.TAKE_NOTE" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".osm.GpsDataPropertiesActivity">
			<intent-filter>
				<action android:name="eu.hydrologis.geopaparazzi.osm.GPSLOG_PROPERTIES" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".osm.GpsDataListActivity">
			<intent-filter>
				<action android:name="eu.hydrologis.geopaparazzi.osm.GPSLOG_DATALIST" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".osm.MapDataPropertiesActivity">
			<intent-filter>
				<action android:name="eu.hydrologis.geopaparazzi.osm.MAPDATAPROPERTIES" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".osm.MapDataListActivity">
			<intent-filter>
				<action android:name="eu.hydrologis.geopaparazzi.osm.MAPSDATALIST" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".osm.OsmActivity">
			<intent-filter>
				<action android:name="eu.hydrologis.geopaparazzi.osm.VIEW_IN_OSM" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".util.AboutActivity">
			<intent-filter>
				<action android:name="eu.hydrologis.geopaparazzi.util.ABOUT" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".util.DirectoryBrowserActivity">
			<intent-filter>
				<action android:name="eu.hydrologis.geopaparazzi.util.DIRECTORYBROWSER" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".util.InsertCoordActivity">
			<intent-filter>
				<action android:name="eu.hydrologis.geopaparazzi.util.INSERT_COORD" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".preferences.PreferencesActivity">
			<intent-filter>
				<action android:name="eu.hydrologis.geopaparazzi.preferences.PREFERENCES" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".chart.ChartActivity">
			<intent-filter>
				<action android:name="eu.hydrologis.geopaparazzi.chart.VIEW_IN_CHART" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".gpx.GpxImportActivity">
			<intent-filter>
				<action android:name="eu.hydrologis.geopaparazzi.gpx.GPXIMPORT" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".osm.OsmTagsActivity">
			<intent-filter>
				<action android:name="eu.hydrologis.geopaparazzi.osm.OSMTAGS" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<activity android:name=".osm.MeasureInfoActivity">
			<intent-filter>
				<action android:name="eu.hydrologis.geopaparazzi.osm.MEASUREMENT_INFO" />
				<category android:name="android.intent.category.DEFAULT" />
			</intent-filter>
		</activity>
		<receiver android:name=".sms.SmsReceiver" android:enabled="true">
			<intent-filter>
				<action android:name="android.provider.Telephony.SMS_RECEIVED" />
			</intent-filter>
		</receiver>
	</application>
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>
	<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"></uses-permission>
	<uses-permission android:name="android.permission.CAMERA"></uses-permission>
	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
	<uses-permission android:name="android.permission.ACCESS_WIFI_STATE"></uses-permission>

	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>

	<uses-permission android:name="android.permission.RECEIVE_SMS"></uses-permission>
	<uses-permission android:name="android.permission.SEND_SMS"></uses-permission>
	<!-- <uses-sdk android:minSdkVersion="3"></uses-sdk> -->
	<uses-sdk android:targetSdkVersion="4" android:minSdkVersion="4"></uses-sdk>
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
	<supports-screens android:smallScreens="true"
		android:normalScreens="true" android:largeScreens="true"
		android:anyDensity="true" android:resizeable="true"></supports-screens>


	<uses-permission android:name="android.permission.ACCESS_MOCK_LOCATION"></uses-permission>
</manifest> 